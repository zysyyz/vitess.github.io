<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title><%= page_title() %></title>
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<meta name="baidu-site-verification" content="05MX5vANvW" />

<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/outdated-browser/1.1.5/outdatedbrowser.min.css">

<% if (has_config('head.open_graph')) { %>
    <%- open_graph({
        twitter_id: get_config('head.open_graph.twitter_id', ''),
        twitter_site: get_config('head.open_graph.twitter_site', ''),
        google_plus: get_config('head.open_graph.google_plus', ''),
        fb_admins: get_config('head.open_graph.fb_admins', ''),
        fb_app_id: get_config('head.open_graph.fb_app_id', '')
    }) %>
<% } %>

<% if (has_config('rss')) { %>
<link rel="alternative" href="<%- get_config('rss') %>" title="<%= get_config('title') %>" type="application/atom+xml">
<% } %>

<% if (has_config('favicon')) { %>
<link rel="icon" href="<%- get_config('favicon') %>">
<% } %>

<script src="//cdn.bootcss.com/pace/1.0.2/pace.min.js"></script>
<link href="//cdn.bootcss.com/pace/1.0.2/themes/pink/pace-theme-flash.css" rel="stylesheet">

<link rel="stylesheet" href="//fonts.lug.ustc.edu.cn/css?family=Ovo|Source+Code+Pro">
<link rel="stylesheet" href="//cdn.bootcss.com/bulma/0.6.2/css/bulma.min.css">

<% if (!(has_config('plugins.gallery') && get_config('plugins.gallery') === false)) { %>
<link rel="stylesheet" href="//cdn.bootcss.com/lightgallery/1.6.10/css/lightgallery.min.css">
<link rel="stylesheet" href="//cdn.bootcss.com/justifiedGallery/3.7.0/css/justifiedGallery.min.css">
<% } %>

